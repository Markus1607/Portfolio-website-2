<% include ./partials/header.ejs %>

    <% blogs.forEach(function(blog){ %>

        <a href="/blogs/<%= blog._id %>" class="btn btn-primary text-center">
          <h4>  <%= blog.createdAt.toDateString() %> </h4>
          <h1> <%= blog.title %> </h1>
          <p>  <%= blog.details %> </p>
        </a>

    <% }) %>

